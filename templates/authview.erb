<%
lstr = ""
if !@community.nil?
    lstr = 'authcommunity'
    ltype = @community
elsif !@user.nil?
    lstr = 'authuser'
    ltype = @user
else
    lstr = 'authgroup'
    ltype = @group
end
lstr = "#{lstr} #{@types}"

if !@model.nil? && !@model.empty?
    lstr = "#{lstr} -s #{@model}"
end

lstr = "#{lstr} #{ltype}"

if !@source.nil? && !@source.empty?
    lstr = "#{lstr} #{@source}"
elsif !@level.nil? && !@level.empty?
    lstr = "#{lstr} #{@level}"
end

unless @oid.nil?
    lstr = "#{lstr} #{@oid}"
end

unless @view.nil?
    lstr = "#{lstr} -V #{@view}"
end
-%>
<%= lstr %>
